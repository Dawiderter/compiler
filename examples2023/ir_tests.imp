PROCEDURE fib(T f, n) IS
  i[100], j, k, l
IN
  f[0] := 0;
  f[1] := 1;
  j := 2;
  WHILE j <= n DO
      k := j - 1;
      l := k - 1;
      f[j] := f[k] + f[l];
      j:=j+1;
  ENDWHILE
END

PROGRAM IS
    f[100], n
IN
    READ n;
    fib(f, n);
    WRITE f[n];
END
